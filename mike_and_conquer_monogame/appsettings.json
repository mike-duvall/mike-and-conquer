


{
  "Serilog": {

    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        //"Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "mike_and_conquer_monogame": "Information",
        //        "mike_and_conquer_monogame.main.MainProgram": "Information",
        //        "commands.DummyClass": "Information",
        "commands": "Information"
        //        "LogTest": "Information"

      }
    },
    "Enrich": [
      "FromLogContext"
    ],

    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Debug",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3}] ({SourceContext}.{Method}) {Message:lj} {NewLine}"
        }
      }
    ]
  }
}